{
  "expo": {
    "name": "PropertyBaku",
    "slug": "PropertyBaku",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.propertybaku.app",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Əmlak şəkillərini seçmək üçün foto kitabxanaya giriş lazımdır",
        "NSCameraUsageDescription": "Əmlak şəkillərini çəkmək üçün kameraya giriş lazımdır"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.propertybaku.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Əmlak şəkillərini seçmək üçün foto kitabxanaya giriş lazımdır",
          "cameraPermission": "Əmlak şəkillərini çəkmək üçün kameraya giriş lazımdır"
        }
      ],
      [
        "expo-localization",
        {
          "fallbackLocale": "az",
          "supportedLocales": ["az", "ru", "tr", "en"]
        }
      ]
    ]
  }
}
